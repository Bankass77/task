<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
"http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="helloForm"
			type="com.strutstutorial.form.HelloForm">
		</form-bean>
	</form-beans>
	<action-mappings>
		<action path="/tasks"
			type="ml.taskmanager.action.TaskListAction" name="taskList">
			<forward name="succes" path="/task-list.jsp"></forward>
		</action>
	</action-mappings>

</struts-config>